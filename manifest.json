{
    "manifest_version": 2,
    "name" : "Chrome extn",

    "description" : "Link launcher & Ad-blocker" ,
    
    "version": "1.0.0",
    
    "icons" :{"128": "icon_128.png"},
    
    "browser_action":{
         "default_icon" : "icon.png",
         "default_popup" : "popup.html"
     },
    
     "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    
     "background":{
         "scripts":["background.js"]
     }
}